{
  "expo": {
    "name": "Tydii",
    "slug": "tydii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon.png",
    "scheme": "tydii",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "associatedDomains": ["applinks:tydii.io","applinks:dev.tydii.io"],
      "bundleIdentifier": "com.tydii.customer",
      "config": {
        "googleMapsApiKey": "AIzaSyCLuu2pgsYBcu-iQ8i1i1vlaqz2nLVgsco"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appicon.png",
        "backgroundColor": "#ffffff",
        "resizeMode": "contain"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tydii.customer",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCLuu2pgsYBcu-iQ8i1i1vlaqz2nLVgsco"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "tydii.io",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "dev.tydii.io",
              "pathPrefix": "/"
            },
            {
              "scheme": "tydii"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
